{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/assets/*", "/static/*"],
  "routes": [
    {
      "src": "/n20/*",
      "dst": "/docs/n20/$1"
    },
    {
      "src": "/b48/*",
      "dst": "/docs/b48/$1"
    },
    {
      "src": "/tuning/*",
      "dst": "/docs/tuning/$1"
    },
    {
      "src": "/maintenance/*",
      "dst": "/docs/maintenance/$1"
    },
    {
      "src": "/*",
      "status": 404,
      "dst": "/404.html"
    }
  ]
} 